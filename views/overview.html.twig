{% extends 'base.html.twig' %}

{% block body %}
    <ul class="thumbnails">
        {% for gallery in galleries %}
            <li class="span3">
                <a href="#" class="thumbnail" title="{{ gallery }}">
                    <img src="http://placekitten.com/300/200" alt="">
                </a>
            </li>
        {% else %}
            <h1>No galleries where found</h1>
        {% endfor %}
    </ul>
{% endblock %}

